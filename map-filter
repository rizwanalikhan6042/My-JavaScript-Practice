//map
let arr=[2,4,6,8,9]
let maptry=arr.map(function(h){
    return h+2;
})
// console.log(maptry)

let radius=[2,3,4,5,6,7]
let maparea=radius.map(function(r){
    return Math.PI*r*r;
})
console.log(maparea);
let mapcircum=radius.map(function(r){
    return 2*Math.PI*r;
})
console.log(mapcircum);
let even=arr.filter(function(num){
    return num%2==0;
})
console.log(even)
const trans=[-2,45,6,7,8,-9]
let positive=trans.filter(function(num){
    return num>0;
})
console.log(positive);
